{
  "name": "syncenv",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.mjs",
  "keywords": [
    "env",
    "envrc",
    "archive"
  ],
  "scripts": {
    "test": "bun test",
    "build": "bun build:esm",
    "build:esm": "cross-env NODE_ENV=\"production\" bun build --target=node --minify --sourcemap=external --splitting ./src/index.ts --outdir=./dist",
    "build:watch": "bun build:esm -- --watch",
    "fmt": "prettier --write ./src/"
  },
  "files": [
    "bin",
    "dist"
  ],
  "types": "dist/index.d.ts",
  "bin": {
    "syncenv": "./bin/syncenv.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MOCHI-inc-JAPAN/syncenv.git"
  },
  "author": "tkow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MOCHI-inc-JAPAN/syncenv/issues"
  },
  "homepage": "https://github.com/MOCHI-inc-JAPAN/syncenv#readme",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.90",
    "bun-types": "^1.0.3",
    "cross-env": "^7.0.3",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "typescript": "latest"
  },
  "optionalDependencies": {
    "@google-cloud/secret-manager": "*"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "bun fmt"
